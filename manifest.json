{
    "manifest_version": 3,
    "name": "Bookmarks miniatures",
    "version": "1.0",
    "description": "Extension qui affiche les favoris de l'utilisateur sous forme de miniature.",
    "permissions": [
        "bookmarks"  
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_page": "favoris.html"
    },
        "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}
  