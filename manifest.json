{
    "manifest_version": 3,
    "name": "Favoris Chrome",
    "version": "1.0",
    "description": "Affiche les favoris de l'utilisateur dans une extension",
    "permissions": [
      "bookmarks"  
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
      "default_page": "favoris.html"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
}
  